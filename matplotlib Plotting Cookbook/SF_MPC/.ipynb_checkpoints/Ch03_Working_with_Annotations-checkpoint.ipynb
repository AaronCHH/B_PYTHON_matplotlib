   "source": [
    "# %load Chapter3/08.py\n",
    "import numpy\n",
    "import matplotlib.pyplot as plot\n",
    "\n",
    "X = numpy.linspace(-4, 4, 1024)\n",
    "Y = .25 * (X + 4.) * (X + 1.) * (X - 2.)\n",
    "\n",
    "plot.annotate('Brackmard minimum',\n",
    "              ha = 'center', va = 'bottom',\n",
    "              xytext = (-1.5, 3.),\n",
    "              xy = (0.75, -2.7),\n",
    "              arrowprops = { 'facecolor' : 'k', 'shrink' : 0.05 })\n",
    "\n",
    "plot.plot(X, Y, c = 'k')\n",
    "plot.show()\n"
   ]

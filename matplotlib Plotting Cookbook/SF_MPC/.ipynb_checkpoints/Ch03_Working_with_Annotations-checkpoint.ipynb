   "source": [
    "# %load Chapter3/07.py\n",
    "import numpy\n",
    "import matplotlib.pyplot as plot\n",
    "\n",
    "X = numpy.linspace(-4, 4, 1024)\n",
    "Y = .25 * (X + 4.) * (X + 1.) * (X - 2.)\n",
    "\n",
    "box = {\n",
    "\t'facecolor' : '.75',\n",
    "\t'edgecolor' : 'k',\n",
    "  'boxstyle'  : 'round'\n",
    "}\n",
    "\n",
    "plot.text(-0.5, -0.20, 'Brackmard minimum', bbox = box)\n",
    "\n",
    "plot.plot(X, Y, c = 'k')\n",
    "plot.show()\n"
   ]

    "N = 16\n",
    "\n",
    "for i in range(N):\n",
    "    plt.gca().add_line(plt.Line2D((0, i), (N - i, 0), color = '.75'))\n",
    "\n",
    "plt.grid(True)\n",
    "plt.axis('scaled')\n",
    "plt.show()"

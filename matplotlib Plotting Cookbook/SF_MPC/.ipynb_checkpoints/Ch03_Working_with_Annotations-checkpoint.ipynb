   "source": [
    "# %load Chapter3/09.py\n",
    "import numpy\n",
    "import matplotlib.pyplot as plot\n",
    "\n",
    "X = numpy.linspace(0, 6, 1024)\n",
    "Y1 = numpy.sin(X)\n",
    "Y2 = numpy.cos(X)\n",
    "\n",
    "plot.xlabel('X')\n",
    "plot.xlabel('Y')\n",
    "\n",
    "plot.plot(X, Y1, c = 'k',             lw = 3., label = 'sin(X)')\n",
    "plot.plot(X, Y2, c = '.5', ls = '--', lw = 3., label = 'cos(X)')\n",
    "\n",
    "plot.legend()\n",
    "plot.show()\n"
   ]

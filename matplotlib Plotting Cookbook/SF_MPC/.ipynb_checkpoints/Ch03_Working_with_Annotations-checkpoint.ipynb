    "# %load Chapter3/10.py\n",
    "import numpy\n",
    "import matplotlib.pyplot as plot\n",
    "\n",
    "X = numpy.linspace(-4, 4, 1024)\n",
    "Y = .25 * (X + 4.) * (X + 1.) * (X - 2.)\n",
    "\n",
    "plot.plot(X, Y, c = 'k')\n",
    "\n",
    "plot.grid(True, lw = 2, ls = '--', c = '.75')\n",
    "plot.show()\n"
   ]
